<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.uemotoakira.application04_2">

    <permission
        android:name="com.example.uemotoakira.appplication04_2.SubActivityY">
    </permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:permission="com.example.uemotoakira.appplication04_2.SubActivityY">
        android:theme="@style/AppTheme">
        <activity android:name=".SubActivityY">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

            <!-- 暗黙のIntentによって実行されるための設定 -->
            <intent-filter>
                <action android:name="com.example.uemotoakira.appplication04_2.SubActivityY" />
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>

        </activity>
    </application>

</manifest>